{
    "name": "bilalbaraz/laravel-kibana",
    "description": "This allows you to use your kibana api",
    "keywords": ["laravel", "kibana"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bilal Baraz",
            "email": "bilalbaraz@windowslive.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2.5",
        "guzzlehttp/guzzle": "^6.5",
        "symfony/var-dumper": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Bilalbaraz\\LaravelKibana\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bilalbaraz\\LaravelKibana\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bilalbaraz\\LaravelKibana\\KibanaServiceProvider"
            ]
        }
    }
}
